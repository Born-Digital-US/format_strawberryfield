<div class="format-strawberryfield-pdfs-controller">
    <div class="pages">Pages: <span id="{{ item.id }}-pagenum">1</span>&nbsp;of <span id="{{ item.id }}-pagecount"></span></div>
    <button id="{{ item.id }}-prev" type="button" title="{{ 'Previous'|t }}" class="button btn btn-secondary">Previous</button>
    <button id="{{ item.id }}-next" type="button" title="{{ 'Next'|t }}" class="button btn btn-secondary">Next</button>
</div>
